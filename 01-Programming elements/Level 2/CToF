import java.lang.*;
import java.util.*;
class Level2 {
public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
float c;
System.out.println("Enter the temperature in celcius");
c=sc.nextFloat();
float f=(c*(9/5)) + 32;
System.out.println("The " + c + " in celcius is " + f + " in farenheit.");
}
}