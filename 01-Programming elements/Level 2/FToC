import java.lang.*;
import java.util.*;
class Level2 {
public static void main(String[] args) {

Scanner sc = new Scanner(System.in);
float f;
System.out.println("Enter the temperature in celcius");
f=sc.nextFloat();
double c=(f-32)*5/9;
System.out.println("The " + f + " in fareheit is " + c + " in celcius.");
}
}